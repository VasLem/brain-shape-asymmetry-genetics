	keywords = {Adult,Aged,Biological Specimen Banks,Cathy Philippe,Cerebral Cortex / diagnostic imaging,Cerebral Cortex / physiology*,Connectome*,Endophenotypes*,Female,Gene Expression / physiology,Genome-Wide Association Study*,Humans,Language*,MEDLINE,Magnetic Resonance Imaging,Male,Middle Aged,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Nerve Net / diagnostic imaging,Nerve Net / physiology*,Non-U.S. Gov't,PubMed Abstract,Research Support,Vincent Guillemot,Yasmina Mekki,doi:10.1016/j.neuroimage.2021.118795,pmid:34929384},
	month = {apr},
