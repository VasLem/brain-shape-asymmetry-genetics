	month = {sep},
	number = {10},
	pages = {895--907},
	pmid = {26287746},
	publisher = {New England Journal of Medicine (NEJM/MMS)},
	title = {{FTO Obesity Variant Circuitry and Adipocyte Browning in Humans }},
	url = {https://www.nejm.org/doi/10.1056/NEJMoa1502214},
	volume = {373},
	year = {2015}
