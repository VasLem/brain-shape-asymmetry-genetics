	title = {{Brain scans from 21,297 individuals reveal the genetic architecture of hippocampal subfield volumes}},
	url = {/pmc/articles/PMC6445783/ /pmc/articles/PMC6445783/?report=abstract https://www.ncbi.nlm.nih.gov/pmc/articles/PMC6445783/},
	volume = {25},
