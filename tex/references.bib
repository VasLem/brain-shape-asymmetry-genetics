	keywords = {Annika L Gable,Christian von Mering,Damian Szklarczyk,Databases,MEDLINE,NCBI,NIH,NLM,National Center for Biotechnology Information,National Institutes of Health,National Library of Medicine,Non-U.S. Gov't,PMC7779004,Protein Interaction Mapping*,Protein*,Proteins / genetics*,PubMed Abstract,Research Support,User-Computer Interface,doi:10.1093/nar/gkaa1074,pmid:33237311},
	month = {jan},
